    <div class="add-theme-btn d-flex justify-content-end px-7">
      <button type="button" class="btn btn-primary btn btn-primary btn-sm ms-auto" data-bs-toggle="modal"
        data-bs-target="#exampleModal">
        Add category
      </button>
    </div>

    <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog">
        <div class="modal-content p-5">

          <section class="add-data">
            <div class="text-center">
              <form method="post" action="/categoryAdd" enctype="multipart/form-data">
                <div>
                  <label for="theme-name">Theme name : </label>
                  <input type="text" id="theme-name" name="category" placeholder="Enter theme name">
                </div>
                <div>
                  <label for="min-price">Theme price : </label>
                  <input type="text" id="min-price" name="minPrice" placeholder="Enter Minimum Theme price">
                </div>
                <div class="drop-zone">
                  <span class="drop-zone__prompt">Drop thumb img or click to upload</span>
                  <input type="file" name="img" >
                </div>
                <div>
                  <input type="submit" id="submit-theme" value="submit">
                </div>
              </form>
            </div>
          </section>



        </div>
      </div>
    </div>
    <!-- add-btn-end  -->

