        <!-- user tables start  -->
        <div class="container-fluid py-4">
          <div class="row">
            <div class="col-12">
              <div class="card mb-4">
                <div class="card-header pb-0">
                  <h6>Cetegory List</h6>
                </div>
                <div class="card-body px-0 pt-0 pb-2">
                  <div class="table-responsive p-0">
                    <table class="table align-items-center mb-0">
                      <thead>
                        <tr>
                          <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 text-center">
                            Sr.
                            No.</th>
                          <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 text-center ">
                            Name
                          </th>

                          <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">
                            Minimuum Price
                          </th>

                          <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">
                            Action
                          </th>
                        </tr>
                      </thead>
                      <tbody>


                        <!--1.  Sr No -->

                        <%category.map((val,i)=>{i++%>
                          <tr>
                            <td class="align-middle text-center">
                              <input type="hidden" name="id" value="<%=val.id%>">
                              <span class="text-secondary text-xs font-weight-bold">
                                <%=i%>
                              </span>
                            </td>
                            <!-- 2. name  -->
                            <td>
                              <div class="d-flex justify-content-center px-2 py-1 ">
                                <div>
                                  <img src="<%=val.img%>" class="avatar avatar-sm me-3" alt="user1">
                                </div>
                                <div class="d-flex flex-column justify-content-center">
                                  <h6 class="mb-0 text-sm">
                                    <%=val.category%>
                                  </h6>
                                </div>
                              </div>

                            </td>
                            <td class="text-center">
                              <h6>
                                <%=val.minPrice%>
                              </h6>
                            </td>
                            <td class="align-middle text-center">
                              <!-- <a href="#" class="text-secondary font-weight-bold text-xs" data-bs-toggle="modal"
                                data-bs-target="#editModal<%= val.id %>">
                                Edit
                              </a> -->
                              <a href="/deletecategory?id=<%=val.id%>" class="text-secondary font-weight-bold text-xs"
                                data-toggle="tooltip" data-original-title="Edit user">
                                Remove
                              </a>
                            </td>
                          </tr>
                          <%})%>
                      </tbody>
                    </table>
                  </div>
                </div>
